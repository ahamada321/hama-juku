---
import Layout from '@src/layouts/Layout.astro';

// Import
import HeroSection from '@components/HeroSection.astro';
import ServicesSection from '@components/ServicesSection.astro';
import AboutSection from '@components/AboutSection.astro';
import PortfolioSection from '@components/PortfolioSection.astro';
import ReviewsSection from '@components/ReviewsSection.astro';
import CTA from '@components/CTA.astro';
import FaqSection from '@components/FaqSection.astro';
---

<Layout>
  <HeroSection />
  <ServicesSection />
  <AboutSection />
  <PortfolioSection />
  <ReviewsSection />
  <CTA
    title="ハマ塾フル版 (完全版)"
    description="断片的な知識を一つの戦略にまとめよう。 最短で突き抜けるなら【フル版】"
    ctaText="モニター価格で販売中です"
    ctaLink="/master"
  />
  <FaqSection />
</Layout>