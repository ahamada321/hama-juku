---
import Layout from '@src/layouts/Layout.astro';

// Import
import HeadingPage from '@src/components/HeadingPage.astro';
import CTA from '@components/CTA.astro';
import ReviewsSection from '@components/ReviewsSection.astro';
import FaqSection from '@components/FaqSection.astro';
---

<Layout title="ハマ塾フル版 (完全版) | ハマ塾">
  <HeadingPage
    position='absolute',
    title="ハマ塾フル版 (完全版)"
    description="新しい試みにつき、現在は少人数制でサービスを提供しております。次回の募集再開時には、優先的にご案内いたします。"
  />
  <CTA
    title="申し込み完了しました！"
    description="次回の募集再開時には、優先的にご案内いたします。"
    ctaText="トップページへ戻る"
    ctaLink="/"
  />
  <ReviewsSection />
  <FaqSection />
</Layout>